      <h1 style = "text-align:center">LRR Demonlist Adding Level Form</h1>

    <form action="/addlevel" method="post">
      <div style="display: grid; place-items: center;">
        <label>Name</label>
        <br>
        <input type="text" placeholder="name" name="name" required></input>
        <br></br>
        <label>ytcode</label>
        <br>
        <input type="text" placeholder="ytcode" name="ytcode" required></input>
        <br></br>
        <label>minimumPercent (only if level is mainlist)</label>
        <br>
        <input type="number" placeholder="minimumPercent" name="minimumPercent"></input>
        <br></br>
        <label>Publisher</label>
        <br>
        <input type="text" placeholder="publisher" name="publisher" required></input>
        <br></br>
         <label>Add At #?</label>
        <br>
        <input type="number" placeholder="placement" name="placement" required></input>
        <br></br>
        <p style="font-size: 30px;">(Completion)</p>
        <label>Name</label>
        <br>
        <input type="text" placeholder="username" name="username" required></input>
        <br></br>
        <label>Link</label>
        <br>
        <input type="text" placeholder="link" name="link" required></input>
        <br></br>
        <label>Hertz</label>
        <br><br>
        <input type="text" placeholder="hertz" name="hertz" required></input>
        <br></br>
        <button type="submit" style="border-radius: 15px; font-size: 40px;">Submit</button>
        <br></br>
      </div>
    </form>

