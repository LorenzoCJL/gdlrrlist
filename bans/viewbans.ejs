  <body class = "" id="wesure">
    <!--<body style="background-color:white;">-->
      
    <%- include("../htdocs/navbar.ejs") %>

    <!-- <p><a href = "leaderboard.html">Leaderboard page</a></p>
    <p><a href = "Extendedlist.html">Extended page</a></p> -->
      <h3 style="text-align: center;">Want to ban someone? Ban them here: <a href="https://gdlrrlist.com/ban">https://gdlrrlist.com/ban</a></h3>
     <h1 style = "text-align:center">LRR Demon List Bans</h1>
      <p style="text-align: center;">Currently, there are <%= locals.amount %> bans. <%= locals.amount != 0 ? "View bans by clicking on one of them" : "You are all caught up!" %></p>
        <div id="bans" style="display: grid; place-items: center;"></div>
    <!-- scripts to include -->
     <script>
       var div = document.getElementById("bans")
       for(let i = 0; i < <%= locals.amount %>; i++) {
         let gg = "<%= locals.text %>".split(",")[i]
         div.innerHTML += `<a href="/ban/${i+1}"><button style="border-radius: 5px;">- Ban ${i+1}: ${gg}</button></a><br>`
       }
      (() => {
    if(localStorage.getItem("theme") == "dark") {
      document.getElementById("wesure").className = "dark-theme"
    } else {
      document.getElementById("wesure").className = ""
    }
  })()
</script>
    <script src="https://gdlrrlist.com/JS/dropdown.js"></script><script src="https://gdlrrlist.com/JS/darkbutton.js"></script>
