      <h1 style = "text-align:center">LRR Demon List User Banning Form</h1>

    <form action="/ban" method="post">
      <div style="display: grid; place-items: center;">
        <label>Username</label>
        <br>
        <input type="text" placeholder="Username" name="username" required></input>
        <br></br>
        <label>Ban Level</label>
        <br>
        <select name="ban_time">
  <option value="1">Level 1 (90 days)</option>
  <option value="2">Level 2 (180 days)</option>
  <option value="3">Level 3 (1 year)</option>
  <option value="4">Level 4 (permanent)</option>
</select>
        <br></br>
        <label>Reason?</label>
        <br>
        <input type="text" placeholder="Reason" name="reason" required></input>
        <br></br>
        <button type="submit" style="border-radius: 15px; font-size: 40px;">Submit</button>
        <br></br>
      </div>
    </form>

