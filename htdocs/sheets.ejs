     <div class="search-wrapper">
    <label for="search">Search</label>
    <input type="search" id="search" data-search>
  </div>
  <div class="user-cards" data-user-cards-container></div>
  <template data-user-template>
    <div class="card">
      <div class="header" data-header></div>
      <div class="body" data-body></div>
    </div>
  </template>
     <div style="display: grid; place-items: center;">
       <p>Want to make a new sheet or go to a sheet? Type the name for it here:</p>
       <input name="name" id="name"></input><p id="error-message"></p><button type="button" onclick="postREQ()">Submit</button>
     </div>
    <!-- scripts to include -->
     <script>
       async function postREQ() {
         let name = document.getElementById("name").value
         let dataTwo = await fetch("https://gdlrrlist.com/api/helper/sheets", {
        method: "POST",
      headers: {  
    'Content-Type': 'application/json'
   },
   body: JSON.stringify({
     name
   })
})
         let json = await dataTwo.json()
      if(dataTwo.status == 400) {
        document.getElementById("error-message").innerText = json.message
      } else {
        window.location.href = `https://gdlrrlist.com/sheets/${name}`
      }
    }
       let data = <%- JSON.stringify(locals.everything) %>;
    </script>
    <script src="JS/dropdown.js"></script><script src="JS/darkbutton.js?v=2022-07-30"></script>
    <script src="https://gdlrrlist.com/extrascripts/sheets.js"></script>
    </script>
  </body>
